{
  "name": "paper-kit-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "classnames": "2.2.6",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "nouislider": "13.1.5",
    "react": "16.8.6",
    "react-bootstrap-switch": "15.5.3",
    "react-data-grid": "^6.1.0",
    "react-datetime": "2.16.3",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "reactstrap": "8.0.0",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "set NODE_ENV=development&&set HTTPS=true&&react-scripts start",
    "build-dev": "set NODE_ENV=production&&set GENERATE_SOURCEMAP=true&&react-scripts build&&npm run build-rename",
    "build": "set NODE_ENV=production&&set GENERATE_SOURCEMAP=false&&react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css",
    "minify-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/paper-kit.scss src/assets/css/paper-kit.css --source-map true",
    "comment": "prestart: @powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./certicates.ps1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "3.5.2"
  }
}
